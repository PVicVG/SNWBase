 // Horizontal limits:
    limit_left  = 0;
    limit_right = room_width;
    
 // Vertical Borders:
    border_top    = y-32;
    border_bottom = y+32;
    
 // Vertical limits:
    limit_top    = 0;
    limit_bottom = room_height;
    
 // Camera handle:
    cam_handle = obj_player;
