            <kind>1</kind>
            <string>/// State 1 - Normal
    if (rival_state == 1)
    {
        // Move Right
            physics_move_right();
        
        // Jump
            if (wallR or instance_place(x, y, obj_rival_flag_jump))
            {
                physics_jump()
            }
        
        // Dashing
            // Start Dashing
                if instance_place(x, y, obj_rival_flag_dash) &amp;&amp; !dashing
                {
                    aud_play_simple_inview(player_spindash_release)
                    x_speed = 7;
                    dashing = true;
                    dash_timer = 70;   
                }    
                
            // Handle Dashing
                if dashing
                {
                    // Set Speed
                        x_speed = 7;
                        
                    // Destroy Spikes 
                        var spike = instance_place(x + x_speed + 2, y, par_spike);
                        if spike
                        {
                            with spike
                            {
                                scr_tear_apart()
                                instance_destroy()
                            }
                            
                            aud_play_simple_inview(general_crumble);
                        }
                
                    // Handle Timer
                        if dash_timer &gt; -1
                        {
                            dash_timer -=1
                        }
                        
                        
                        if dash_timer == -1
                        {
                            dashing = false;
                        }
                 }   
                 
        // Spring
            var spring = instance_place(x, bbox_bottom + 1, par_spring);
            if spring
            {
                y_speed = -spring.spring_strength;
                spring.spring_frame = 1;
                aud_play_simple_inview(general_spring)
            }
        
        // Animations
            // Jump
                if !ground
                {
                    sprite_index = spr_rival_roll;
                }
        
            // Walk      
                if ground
                {
                    if !dashing
                    {
                    sprite_index = spr_rival_walk;
                    }
                    else
                    {
                    sprite_index = spr_rival_dash;
                    }
                }
        
        
    }
</string>
