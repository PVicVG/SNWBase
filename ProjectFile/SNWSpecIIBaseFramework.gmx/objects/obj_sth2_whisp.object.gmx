{
 ireset   = 100;
 alarm[0] =  60;
 image_xscale = choose(1,-1,-1,1)
};
};
else speed = 0;

/* Don't go underwater! */
if( y &gt; global.water_position )
{
 near_target = false;
 ireset = 50;
 alarm[0] = 60;
};
