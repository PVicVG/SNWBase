        {   
            // Move in
                if (message_x &lt; start_message_x)
                {
                    message_x += 2;
                }
            
            // Fade in
                if (image_alpha &gt; 0)
                {
                    message_alpha -= 0.125;
                }
        
            // Go to the Next State if finished.
                if (message_alpha &lt;= 0 &amp;&amp; message_x &gt;= start_message_x)
