            <kind>1</kind>
            <string>/// Deactivate instances.

 // Large region: this is for tiles since those are the most important. For this region we use the screen size * 2 (320x224 &gt; 640x448)
 // We want to make sure that these are activated first to prevent that other objects get stuck inside them.
    instance_deactivate_object(obj_destructible);
    instance_deactivate_object(par_terrain);    
    instance_deactivate_object(par_solid);     
    instance_activate_object(obj_indestructible);
    instance_activate_object(obj_monitor_collision);
    instance_activate_object(par_swing);
    instance_activate_object(par_moving_platform);
    instance_activate_object(obj_sth3_catakillar);
    instance_activate_object(obj_sth3_catakillar_body);
    instance_activate_object(obj_sth3_catakiller_electro_ring);
    instance_activate_object(obj_spring_follow);
    instance_activate_object(par_boss);
    instance_activate_region(view_xview[0] - global.screen_width, view_yview[0] - global.screen_height, view_wview[0] + global.screen_width * 2, view_hview[0] + global.screen_height * 2, true);
    
 // Deactivate instances that need to be activated in the middle region:
    instance_deactivate_object(obj_ring);
    instance_deactivate_object(par_enemy);
    instance_deactivate_object(obj_physics_parent);
    instance_activate_object(par_rival_racer);
        
 // Middle region: This is for other objects that we want to spawn next. We have to do this twice.
 // Once using view_xview/view_yview and another one using player coordinates. We do this to make sure that when the player outruns the camera that instances are active inside the camera view and near the player.
    var player;
    player = instance_exists(obj_player);
    var rival;
    rival     = instance_exists(par_rival_racer);
    var boss;
    boss     = instance_exists(par_boss);

    // Activate stuff in-view
        instance_activate_region(view_xview[0]-64, view_yview-64, view_wview[0]+128, view_hview[0]+128, true);
    
    // Activate Region Around Player
        if(player == true)
        {
           instance_activate_region(obj_player.x-global.screen_width/2, obj_player.y-global.screen_height/2, global.screen_width, global.screen_height, true);    
        }
        
    // Activate Region Around Rival Racers
        if(rival == true)
        {
           instance_activate_region(par_rival_racer.x-global.screen_width/2, par_rival_racer.y-global.screen_height/2, global.screen_width, global.screen_height, true);    
        }

    // Activate Region Around Bosses
        if(boss == true)
        {
           instance_activate_region(par_boss.x-global.screen_width/2, par_boss.y-global.screen_height/2, global.screen_width, global.screen_height, true);    
        }
</string>
