            <kind>1</kind>
            <string>/// Hitting

    // Exit If Dead
        if (state == -1 or other.state == STATE_DEATH)
        {
            exit;
        }
    
    // Only hit if not invincibile
        if (inv_timer == -1)
        {
            if !(other.harmful)
            {
                with (other)
                {
                    scr_player_hurt()
                }
            }
            else
            {
                inv_timer = 60;
                hp -= 1;
                aud_play_simple(general_enemy_hit)
            }
        }
    
    // Knockback
        x_speed = 5 * sign(x - other.x) 
        y_speed = 5 * sign(y - other.y)
        
        with (other)
        {
            if (other.x != x)
            {
                x_speed = 5 * sign(x - other.x)
            }
            else
            {
                x_speed = 5 * -animation_direction;
            }
            
            y_speed = 5 * sign(y - other.y) 
            
            if (harmful)
            {
                if (ground)
                {
                    state = STATE_ROLL;
                }
                else
                {
                    state = STATE_JUMP;
                }
            }
            
        }
</string>
