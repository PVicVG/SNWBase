        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Vehicle UI.
draw_sprite_ext(spr_centered_gradient, 0, view_xview[0], view_yview[0] + 135, 1, vehicle_ui_alpha, 0, c_white, vehicle_ui_alpha);

draw_set_blend_mode(bm_add);
draw_sprite_ext(spr_center_ball, 0, view_xview[0] + 240, view_yview[0] + 135, vehicle_ui_alpha, vehicle_ui_alpha, 0, c_white, 1);
draw_set_blend_mode(bm_normal);

draw_sprite_ext(spr_UI_cross, vehicle_ui_frame, view_xview[0] + 240, view_yview[0] + 135, vehicle_ui_alpha, vehicle_ui_alpha, 0, c_white, vehicle_ui_alpha);
draw_sprite_ext(spr_available_vehicles, 0, view_xview[0], view_yview[0], vehicle_ui_alpha, vehicle_ui_alpha, 0, c_white, vehicle_ui_alpha);
</string>
