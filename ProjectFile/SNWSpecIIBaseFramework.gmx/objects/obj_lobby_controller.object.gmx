    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Vehicle UI Handle
    
    // Delay
    if vehicle_ui_delay &gt; -1
    {
        vehicle_ui_delay --;
    }
    
    // Release CTRL, Give Vehicles
    if (keyboard_check_released(vk_control) &amp;&amp; vehicle_ui_active &amp;&amp; vehicle_ui_alpha == 1)
    {
        if ((vehicle_ui_frame != 0) or (vehicle_ui_frame == 0 &amp;&amp; instance_exists(par_ride)))
        {
            // Destroy Rides
            if instance_exists(par_ride)
            {
                with (par_ride)
                {
                    aud_play_simple(general_destroy);
                    scr_tear_apart()
                    instance_destroy();
                }
            }
            
            // Create Instance
            if vehicle_ui_frame != 0
            {           
                aud_play_simple(ui_start);
                aud_play_simple(general_appear);
                obj_player.ground = false;
                obj_player.jump_completed = true;
                obj_player.y_speed = -6;
                
                if !instance_place(obj_player.x, obj_player.y - 15, par_solid)
                {
                obj_player.y -= 15;
                }
                
                obj_player.state = STATE_SPRING;
                scr_cam_shake()
                
                if vehicle_ui_frame == 1
                {
                    instance_create(obj_player.x, obj_player.y, obj_rideable_turtle);
                }
                
                if vehicle_ui_frame == 2
                {
                    instance_create(obj_player.x, obj_player.y, obj_rideable_octopus);
                }
                
                if vehicle_ui_frame == 3
                {
                    instance_create(obj_player.x, obj_player.y, obj_rideable_hoverboard);
                }
                
                if vehicle_ui_frame == 4
                {
                    instance_create(obj_player.x, obj_player.y, obj_rideable_cloud);
                }
            }
            
            vehicle_ui_active = false;
        }
    }

    // Activate.
        if (instance_exists(obj_player))
        {
            if (!obj_player.control_lock &amp;&amp; obj_player.visible)
            {
                if (keyboard_check(vk_control))
                {
                    if (!vehicle_ui_active)
                    {
                        vehicle_ui_active = true;
                        aud_play_simple(ui_open);
                    }
                }
                else
                {
                    if (vehicle_ui_active)
                    {
                        vehicle_ui_active = false;
                        aud_play_simple(ui_close);
                    }
                }
            }
            else
            {
                vehicle_ui_active = false;
            }
        }
        else
        {
            vehicle_ui_active = false;
        }
    
    // Active. Do things.
        if (vehicle_ui_active)
        {
           // Fade in
                if (vehicle_ui_alpha &lt; 1)
                {
                    vehicle_ui_alpha += 0.1;
                }
                else
                {
                    // Frame.
                        if (!keyboard_check(global.key_up) &amp;&amp; !keyboard_check(global.key_left) &amp;&amp; !keyboard_check(global.key_right) &amp;&amp; !keyboard_check(global.key_down) &amp;&amp; vehicle_ui_frame != 0 &amp;&amp; vehicle_ui_delay == -1)
                        {
                            aud_play_simple(ui_previous);
                            vehicle_ui_frame = 0;   
                        }
                        
                        if (keyboard_check(global.key_up) &amp;&amp; vehicle_ui_frame != 1)
                        {
                            aud_play_simple(ui_next);
                            vehicle_ui_frame = 1; 
                            vehicle_ui_delay = fac_vehicle_ui_delay;  
                        }
                        
                        if (keyboard_check(global.key_right) &amp;&amp; vehicle_ui_frame != 2)
                        {
                            aud_play_simple(ui_next);
                            vehicle_ui_frame = 2;   
                            vehicle_ui_delay = fac_vehicle_ui_delay;
                        }   
                        
                        if (keyboard_check(global.key_down) &amp;&amp; vehicle_ui_frame != 3)
                        {
                            aud_play_simple(ui_next);
                            vehicle_ui_frame = 3;  
                            vehicle_ui_delay = fac_vehicle_ui_delay; 
                        }            
                        
                        if (keyboard_check(global.key_left) &amp;&amp; vehicle_ui_frame != 4)
                        {
                            aud_play_simple(ui_next);
                            vehicle_ui_frame = 4; 
                            vehicle_ui_delay = fac_vehicle_ui_delay;  
                        }          
                
                    // Stop Player
                        with (obj_player)
                        {
                            player_inputs_null()
                        }
                        
                    
                }
        }
    
    // Not Active. Undo Things.
        if (!vehicle_ui_active)
        {              
            // Fade Out
                if (vehicle_ui_alpha &gt; 0)
                {
                    vehicle_ui_alpha -= 0.1;
                }
        }
</string>
          </argument>
        </arguments>
      </action>
    </event>
