            <string>/// A moving turtle.

    var player_ontop;
    player_ontop = instance_place(x, bbox_top - 2, obj_player);

    if (player_ontop)
    {
        // Image Speed.
            image_speed = 0.20+abs(obj_player.x_speed)/25;
        
        // Snap To Player
            if !instance_place(obj_player.bbox_right + 2, obj_player.y, par_solid) &amp;&amp; !instance_place(obj_player.bbox_left - 2, obj_player.y, par_solid)
            x = obj_player.x;
        
        // Move Up
            if obj_player.input_up &amp;&amp; !instance_place(x, obj_player.bbox_top - 2, par_solid) &amp;&amp; obj_player.ground
            {
                y -= 2;
                obj_player.y -= 2;
            }
        
        // Move Down
            if obj_player.input_down &amp;&amp; !instance_place(x, bbox_bottom + 2, par_solid) &amp;&amp; obj_player.ground
            {
                y += 2;
                obj_player.y += 2;
            }
        
        // Set Direction
            if obj_player.x_speed != 0
            {
                image_xscale = sign(obj_player.x_speed);
            }
    }        

</string>
          </argument>
