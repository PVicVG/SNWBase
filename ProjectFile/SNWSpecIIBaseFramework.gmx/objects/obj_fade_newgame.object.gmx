            <kind>1</kind>
            <string>/// Update Colors.

//  Red:
    if(color_red &lt; 255){
       color_red += 20;
    }

//  Green:
    if(color_green &lt; 255 &amp;&amp; color_red &gt; 102){
       color_green += 20;
    }
        
//  Blue:
    if(color_blue &lt; 255 &amp;&amp; color_green &gt; 102){
       color_blue += 20;
    }

//  End the transition:
    if(color_blue == 255){
       if(image_alpha &lt; 1){
          image_alpha += 0.050;
       }else{
         audio_stop_all();
         instance_destroy()
       }
    }
</string>
